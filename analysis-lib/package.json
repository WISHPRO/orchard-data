{
  "name": "analysis-lib",
  "version": "0.0.0",
  "description": "Command line interface to run the TSV tests",
  "author": "",
  "scripts": {
    "dev": "nodemon api/main.js",
    "test:unit": "NODE_ENV=test ./node_modules/.bin/mocha --recursive test/unit/",
    "test:unit:keep_db": "NODE_ENV=test KEEP_DB=true ./node_modules/.bin/mocha --recursive test/unit/",
    "load-and-run": "node ./dev-scripts/load-and-run",
    "update-status": "node ./dev-scripts/update-status",
    "clear-tables": "node ./dev-scripts/clear-dev-tables",
    "gen-row-report": "node ./src/scripts/gen-row-report",
    "gen-error-report": "node ./src/scripts/gen-error-report",
    "tsv-to-json": "node ./dev-scripts/tsv-to-json",
    "run-filter": "node ./dev-scripts/run-filter"
  },
  "private": true,
  "dependencies": {
    "ajv": "^6.0.0",
    "bluebird": "^3.5.1",
    "body-parser": "^1.18.2",
    "config": "^1.29.4",
    "cors": "^2.8.4",
    "csv-parse": "^2.0.4",
    "csv-parser": "^1.12.0",
    "csv-write-stream": "^2.0.0",
    "express": "^4.16.2",
    "is-my-json-valid": "^2.17.2",
    "lodash": "^4.17.5",
    "minimist": "^1.2.0",
    "morgan": "^1.9.0",
    "musicbrainz": "^0.2.6",
    "sqlite": "^2.9.1"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "chai-http": "^3.0.0",
    "eslint": "^4.16.0",
    "mocha": "^5.0.1",
    "node-rest-client": "^3.1.0",
    "nodemon": "^1.14.11",
    "sinon": "^4.3.0"
  }
}
